    "jasmine-reporters": "^2.2.0"
  },
  "peerDependencies": {
    "enzyme-to-json": "^3.0.1",
    "react": "^16.0.0",
    "react-test-renderer": "^16.0.0",
    "react-dom": "^16.0.0"
  },
  "devDependencies": {
    "jest": "^21.2.1"
  },
  "contributors": [
    "Andres Escobar",
    "Jimmy King"
  ],
  "author": "Andres Escobar <andres.escobar@aexp.com> (https://github.com/anescobar1991)",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/americanexpress/amex-jest-preset-react.git"
  },
  "files": [
    "jest-setup.js",
    "react-specific-preset.js",
    "scripts/create-jest-preset.js"
  ],
  "scripts": {
    "test": "jest",
    "postinstall": "node scripts/create-jest-preset.js"
