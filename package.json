{
  "name": "amex-jest-preset-react",
  "description": "An opinionated Jest preset for React modules",
  "version": "5.0.1",
  "keywords": [
    "jest",
    "preset",
    "amex",
    "react",
    "quality"
  ],
  "main": "jest-preset.js",
  "dependencies": {
    "amex-jest-preset": "^5.0.0",
    "enzyme": "^3.0.0",
    "enzyme-adapter-react-16": "^1.7.0",
    "identity-obj-proxy": "^3.0.0"
  },
  "peerDependencies": {
    "enzyme-to-json": "^3.0.1",
    "jest": "^23.0.0",
    "react": "^16.0.0",
    "react-test-renderer": "^16.0.0",
    "react-dom": "^16.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^7.5.2",
    "@commitlint/config-conventional": "^7.5.0",
    "githook-scripts": "^1.0.1",
    "jest": ">=23.0.0"
  },
  "contributors": [
    "Andres Escobar",
    "Jimmy King"
  ],
  "author": "Andres Escobar <andres.escobar@aexp.com> (https://github.com/anescobar1991)",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/americanexpress/amex-jest-preset-react.git"
  },
  "files": [
    "jest-setup.js",
    "jest-preset.js"
  ],
  "scripts": {
    "githook:commit-msg": "commitlint --edit $GIT_PARAMS",
    "test": "jest",
    "test:git-history": "commitlint --from origin/master --to HEAD",
    "posttest": "npm run test:git-history"
  },
  "jest": {
    "preset": "amex-jest-preset",
    "coveragePathIgnorePatterns": [
      "<rootDir>/jest-setup.js",
      "<rootDir>/commitlint.config.js"
    ]
  }
}
